<h2>Hello world!</h2>
<p>Sinatra here. Giving you some swanky conent to spice up your day!</p>


<script>

$(document).ready(function(){
  var p = $('p');
  $.ajax({
  type: 'get',
  dateType: 'json',
  url: '/api/json'
  success: function(data) {
    console.log(data);
    $(p).html(data.message);
  },
  error: function(err) {
    console.log(err);
    $(p).html('An error occured');
  }
  });

});


</script>
